<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>GithubClient</RootNamespace>
    <PackageId>GithubClient</PackageId>
    <Version>1.0.0</Version>
    <Authors>atrakic</Authors>
    <Company></Company>
    <PackageDescription></PackageDescription>
    <RepositoryUrl>https://github.com/atrakic/dotnet-nuget-packages</RepositoryUrl>
    <PackageVersion>0.0.2</PackageVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="GitHubJwt" Version="0.0.6" />
    <PackageReference Include="Microsoft.Extensions.Http" Version="9.0.0" />
    <PackageReference Include="NLog" Version="5.3.4" />
    <PackageReference Include="Octokit" Version="13.0.1" />
  </ItemGroup>

</Project>
